<!DOCTYPE html>
<html>
	<head>
	  <title>FoodVendor</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>
	  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	  <script src="https://unpkg.com/masonry-layout@4.0/dist/masonry.pkgd.min.js"></script>
	  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
	</head>
	<body>
	  <header>
	  	<nav>
	  		
		  	<ul>
		      <% if flash[:notice] %>
		        <li class="notice"><%= notice %></li>
		      <% end %>
		      <% if flash[:alert] %>
		        <li class="alert"><%= alert %></li>
		      <% end %>

			    <% if vendor_signed_in? %>
			      <%= link_to 'Vendor Log out', destroy_vendor_session_path,  method: :delete, :class => "change-border__inner" %>
			     <% else %>
			     <%= link_to 'Vendor Sign up', new_vendor_registration_path, :class => "change-border__inner" %>
			     <%= link_to 'Vendor Login', vendor_session_path, :class => "change-border__inner" %>
		     	<% end %>

		     
		    </ul>
		   
	   	</nav>

  	</header>
  


	<%= yield %>

	</body>
</html>
